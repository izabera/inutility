inutility
=========

synopsis                                                    | notes
------------------------------------------------------------+-----------------
base64 [-di] [-w num] [file]                                |
basename [-az] [-s suffix] name [name...]                   |
cal [[month] year]                                          |
cat [-AeEntsTuv] [file...]                                  | todo -b
chroot root [cmd...]                                        |
comm [-123z] file1 file2                                    |
cmp [-ls] [-n byte] file1 [file2 [skip1 [skip2]]]           | todo -b
clear                                                       |
cut [-nsz] [-b list] [-c list] [-f list] [-d sep] [file...] |
date [-Ru] [-D format] [-d time] [-r file] [time]           |
dd [various=options]                                        |
dirname [-z] name [name...]                                 |
echo [-n] [arg...]                                          |
env [-i0] [-u unset...] [-] [VAR=value...] [cmd [args...]]  |
errno [-ls] [signal]                                        |
factor [arg...]                                             |
false                                                       |
file [-hL] [file...]                                        |
getopt [-various] [-options]                                | documented in doc/getopt
grep [-acEFhHilLnqsvwx] [-e arg] [-f file] [arg...]         | todo: -A -B -C and maybe more...
head [-n num] [-num] [-c byte] [-qvz]                       |
kill [-sig] [-l] [-s sig] pid [pid...]                      |
line                                                        |
link file1 file2                                            |
list [-n]                                                   | lists inutilities
mkdir [-pv] dir1 [dir...]                                   | todo: mode
mktemp [-dqu] [-p dir] [template]                           |
nice [-num] [-n num] cmd [args...]                          |
nohup cmd [arg...]                                          |
