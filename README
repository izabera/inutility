rmdir [-pv] dir [dir...]                                    |
seq [-w] [-s arg] [-f arg] arg1 [arg2] [arg3]               |
shuf [-erz] [-i lo-hi] [-n num] [file...]                   |
sleep number[suffix]...                                      |
sort [-bcdfghimnruCMRV] [-o file] [-t sep] [file...]        | todo -k
stat [-Lft] [-c arg] file1 [file...]                        | todo -c %m
sync [-df] [file...]                                        |
tac [file...]                                               | no opts
tee [-ai] [file...]                                         |
test [expr]                                                 |
timeout [-p] [-k num] [-s num] time cmd [args...]           |
touch [-acfm] [-r file] [-t time] [-d date] file [file...]  |
tr [-cCtds] set1 [set2]                                     | no [=equiv=] [x*n]
true                                                        |
truncate [-c] [-r file] [-s byte] file [file...]            |
tty [-s]                                                    |
uname [-amnrsv]                                             |
unshare [-fimnpuU] cmd [args...]                            |
uniq [-icdDu] [-s num] [-w num] [-z] [infile [outfile]]     | todo -f
unlink file [file...]                                       | allows more than 1 file
uptime                                                      | no opts
wc [-wclm] [file...]                                        |
yes [args...]                                               |


